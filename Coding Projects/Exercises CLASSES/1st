<script>
    class Vehicle {
    constructor(passengers, mSpeed, wheelCount){
    this.passengers = passengers;
    this.mSpeed = mSpeed;
    this.wheelCount = wheelCount;
    }
    currentSpeed(cSpeed){
    console.log(cSpeed)
    }
    }
    class Boat extends Vehicle {
    constructor(mDepth){
    super(2, 30, 0);
    this.mDepth = mDepth;
    }
    }
    class Car extends Vehicle {
    constructor(gasType){
    super(5, 80, 4);
    this.gasType = gasType;
    }
    }
